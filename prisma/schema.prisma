model thread {
  id                              String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  created_at                      DateTime  @default(now()) @db.Timestamptz(6)
  owner                           String?   @db.Uuid
  chapter                         String?   @db.Uuid
  host_llm                        host_llm?
  message_message_threadTothread  message[] @relation("message_threadTothread")
  chapter_thread_chapterTochapter chapter?  @relation("thread_chapterTochapter", fields: [chapter], references: [id], onUpdate: NoAction)
  users                           users?    @relation(fields: [owner], references: [id], onUpdate: NoAction)

  @@schema("public")
}

model chapter {
  created_at                               DateTime  @default(now()) @db.Timestamptz(6)
  owner                                    String?   @db.Uuid
  lecture                                  String?   @db.Uuid
  start_date                               DateTime? @db.Timestamptz(6)
  end_date                                 DateTime? @db.Timestamptz(6)
  status                                   status?   @default(NOT_STARTED)
  id                                       String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title                                    String?
  lecture_chapter_lectureTolecture         lecture?  @relation("chapter_lectureTolecture", fields: [lecture], references: [id], onUpdate: NoAction)
  users                                    users?    @relation(fields: [owner], references: [id], onUpdate: NoAction)
  code_code_chapterTochapter               code[]    @relation("code_chapterTochapter")
  lecture_lecture_current_chapterTochapter lecture[] @relation("lecture_current_chapterTochapter")
  thread_thread_chapterTochapter           thread[]  @relation("thread_chapterTochapter")

  @@schema("public")
}

model code {
  id                            String                @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  created_at                    DateTime              @default(now()) @db.Timestamptz(6)
  owner                         String?               @db.Uuid
  chapter                       String?               @db.Uuid
  version                       Int?
  language                      programming_language?
  author                        code_author?
  content                       String?
  chapter_code_chapterTochapter chapter?              @relation("code_chapterTochapter", fields: [chapter], references: [id], onUpdate: NoAction)
  users                         users?                @relation(fields: [owner], references: [id], onUpdate: NoAction)

  @@schema("public")
}

model lecture {
  created_at                                      DateTime      @default(now()) @db.Timestamptz(6)
  owner                                           String?       @db.Uuid
  lecture_info                                    String?       @db.Uuid
  start_date                                      DateTime?     @db.Timestamptz(6)
  end_date                                        DateTime?     @db.Timestamptz(6)
  current_chapter                                 String?       @db.Uuid
  status                                          status?       @default(NOT_STARTED)
  id                                              String        @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  chapter_chapter_lectureTolecture                chapter[]     @relation("chapter_lectureTolecture")
  chapter_lecture_current_chapterTochapter        chapter?      @relation("lecture_current_chapterTochapter", fields: [current_chapter], references: [id], onUpdate: NoAction)
  lecture_info_lecture_lecture_infoTolecture_info lecture_info? @relation("lecture_lecture_infoTolecture_info", fields: [lecture_info], references: [id], onUpdate: NoAction)
  users                                           users?        @relation(fields: [owner], references: [id], onUpdate: NoAction)

  @@schema("public")
}

model lecture_info {
  id                                         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  created_at                                 DateTime  @default(now()) @db.Timestamptz(6)
  title                                      String?
  thumbnail_image                            String?
  chapter                                    String?
  lecture_lecture_lecture_infoTolecture_info lecture[] @relation("lecture_lecture_infoTolecture_info")

  @@schema("public")
}

model llm_module {
  id                                     String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  created_at                             DateTime  @default(now()) @db.Timestamptz(6)
  name                                   String?
  version                                Int?
  description                            String?
  system_prompt                          String?
  message_message_llm_moduleTollm_module message[] @relation("message_llm_moduleTollm_module")

  @@schema("public")
}

model message {
  id                                        String        @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  created_at                                DateTime      @default(now()) @db.Timestamptz(6)
  owner                                     String?       @db.Uuid
  thread                                    String?       @db.Uuid
  role                                      message_role?
  llm_module                                String?       @db.Uuid
  content                                   String?
  tokens                                    Int?
  llm_module_message_llm_moduleTollm_module llm_module?   @relation("message_llm_moduleTollm_module", fields: [llm_module], references: [id], onUpdate: NoAction)
  users                                     users?        @relation(fields: [owner], references: [id], onUpdate: NoAction)
  thread_message_threadTothread             thread?       @relation("message_threadTothread", fields: [thread], references: [id], onUpdate: NoAction)

  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}

enum host_llm {
  cohee
  gpt

  @@schema("public")
}

enum status {
  NOT_STARTED
  IN_PROGRESS
  COMPLETED

  @@schema("public")
}

enum message_role {
  system
  assistant
  user
  tool

  @@schema("public")
}

enum programming_language {
  javascript
  html
  css
  python

  @@schema("public")
}

enum code_author {
  user
  gpt

  @@schema("public")
}
